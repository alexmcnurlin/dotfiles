{
    "name": "Default color scheme for shell prompts",
    "groups": {
        "hostname":                  { "fg": "bright_yellow", "bg": "neutral_orange", "attrs": [] },
        "environment":               { "fg": "light0",        "bg": "faded_green",    "attrs": [] },
        "mode":                      { "fg": "dark1",         "bg": "bright_blue",    "attrs": ["bold"] },
        "cwd":                       { "fg": "light4",        "bg": "dark2",          "attrs": [] },
        "cwd:current_folder":        { "fg": "light4",        "bg": "dark1",          "attrs": ["bold"]   },
        "cwd:divider":               { "fg": "light3",        "bg": "dark2",          "attrs": [] },
        "attached_clients":          { "fg": "light0",        "bg": "faded_green",    "attrs": [] },
        "gitstatus":                 { "fg": "light4",        "bg": "light1",         "attrs": [] },
        "gitstatus_branch":          { "fg": "light4",        "bg": "light1",         "attrs": [] },
        "gitstatus_branch_clean":    { "fg": "dark1",         "bg": "bright_green",   "attrs": [] },
        "gitstatus_branch_dirty":    { "fg": "dark1",         "bg": "bright_orange",  "attrs": [] },
        "gitstatus_branch_detached": { "fg": "light2",        "bg": "neutral_purple", "attrs": [] },
        "gitstatus_behind":          { "fg": "neutral_red",   "bg": "dark1",          "attrs": [] },
        "gitstatus_ahead":           { "fg": "neutral_green", "bg": "dark1",          "attrs": [] },
        "gitstatus_staged":          { "fg": "neutral_aqua",  "bg": "dark1",          "attrs": [] },
        "gitstatus_unmerged":        { "fg": "neutral_red",   "bg": "dark1",          "attrs": [] },
        "gitstatus_changed":         { "fg": "neutral_orange","bg": "dark1",          "attrs": [] },
        "gitstatus_untracked":       { "fg": "neutral_orange","bg": "dark1",          "attrs": [] },
        "gitstatus_stashed":         { "fg": "neutral_blue",  "bg": "dark1",          "attrs": [] },
        "gitstatus:divider":         { "fg": "gray_245",      "bg": "light1",         "attrs": [] }
    },
    "mode_translations": {
        "vicmd": {
            "groups": {
                "mode": {"fg": "dark0", "bg": "light4", "attrs": ["bold"]}
            }
        }
    }
}
